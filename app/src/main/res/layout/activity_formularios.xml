<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_formularios"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ventana_datos"
    tools:context="general.me.edu.dgtmovil.dgtmovil.FormulariosActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/txt_territorio"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:textColor="@android:color/white"
            android:layout_weight="0.15"
            android:text="DIRECCION DE GESTION TERRITORIAL"
            android:textAlignment="center"
            android:textSize="21dp"
            android:gravity="center"/>

        <ListView
            android:id="@+id/lst_formularios"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="25dp"
            android:layout_weight="0.85"
            />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
